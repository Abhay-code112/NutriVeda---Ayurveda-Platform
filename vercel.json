{
  "version": 2,
  "builds": [
    {
      "src": "backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/wsgi.py"
    }
  ],
  "functions": {
    "backend/wsgi.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "installCommand": "pip install -r requirements-vercel.txt"
}

